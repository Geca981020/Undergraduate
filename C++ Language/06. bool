#include <iostream>

using namespace std;

int main()
{
	int score;
	bool decision;

	cout.setf(ios::boolalpha );

	cout << "Enter your score: ";
	cin >> score;
	decision = (score >= 60);
	cout << decision << endl;

	cout << !(false || true) << endl;
	cout << ((5>6) && (7/0)) << endl;
	cout << !(5 && 7) << !(!6) << endl;

	return 0;
}
